{
  "extends": [
    "airbnb",
    "plugin:jest/recommended",
    "prettier",
  ],
  "parser": "babel-eslint",
  "plugins": [
    "emotion",
    "prettier",
  ],
  "rules": {
    "import/no-named-as-default": false,
    "react/jsx-filename-extension": false,
    "prettier/prettier": "error",
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./", "src"],
      },
    },
  },
}
