{
  "name": "@octopusthink/nautilus",
  "version": "0.4.1",
  "description": "Nautilus design system for React",
  "main": "dist/main.js",
  "module": "src/index.js",
  "scripts": {
    "build:package": "cross-env NODE_ENV=production webpack --mode=production",
    "build:styleguide": "cross-env DISABLE_PEER_DEPS_PLUGIN=true NODE_ENV=production styleguidist build",
    "build": "npm run build:package",
    "clean": "shx rm -rf coverage/ dist/",
    "lint:eslint:fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix && npm run lint:stylelint:fix && npm run lint:markdown:fix",
    "lint:markdown": "markdownlint '**/*.md' --ignore node_modules",
    "lint:markdown:fix": "markdownlint '**/*.md' --ignore node_modules --fix",
    "lint:prettier:fix": "prettier -c --write '**/*.js'",
    "lint:prettier": "prettier -c '**/*.js'",
    "lint:stylelint:fix": "stylelint --fix '**/*.js'",
    "lint:stylelint": "stylelint '**/*.js'",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint && npm run lint:markdown",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "prepublish:styleguide-for-version": "npm run build && npm run build:styleguide",
    "prepublish:styleguide": "npm run build && npm run build:styleguide",
    "publish:styleguide-for-version": "cross-env PUBLISH_VERSION_FOLDER=true node bin/publish-gh-pages.js",
    "publish:styleguide": "node bin/publish-gh-pages.js",
    "start": "styleguidist server",
    "test:coverage:watch": "npm run test:coverage -- --watchAll",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:unit:update": "npm run test:unit -- --updateSnapshot",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit": "jest",
    "test": "npm run test:unit:watch"
  },
  "prettier": "@octopusthink/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octopusthink/nautilus.git"
  },
  "author": "Octopus Think",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octopusthink/nautilus/issues"
  },
  "homepage": "https://github.com/octopusthink/nautilus#readme",
  "dependencies": {
    "@reach/combobox": "^0.16.1",
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "feather-icons": "^4.28.0",
    "invariant": "^2.2.4",
    "prop-types": "^15.7.2",
    "react-select": "^4.3.1",
    "shortid": "^2.2.16",
    "shx": "^0.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx-source": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@emotion/babel-plugin": "^11.2.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@emotion/jest": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@octopusthink/eslint-config": "^0.6.0",
    "@octopusthink/prettier-config": "^0.6.1",
    "@reach/router": "^1.3.4",
    "@react-hook/window-size": "^3.0.7",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "acorn": "^8.0.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "history": "^5.0.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-axe": "^3.4.0",
    "lint-staged": "^10.5.4",
    "markdownlint-cli": "^0.26.0",
    "prettier": "^2.2.1",
    "process": "^0.11.10",
    "react": "^17.0.1",
    "react-dev-utils": "^12.0.0-next.37",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-styleguidist": "^11.1.7",
    "shelljs": "^0.8.4",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.5",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1"
  }
}
