{
  "name": "@octopusthink/nautilus",
  "version": "0.4.0",
  "description": "Nautilus design system for React",
  "main": "dist/main.js",
  "module": "src/index.js",
  "scripts": {
    "build:package": "NODE_ENV=production webpack",
    "build:styleguide": "DISABLE_PEER_DEPS_PLUGIN=true NODE_ENV=production styleguidist build",
    "build": "npm run build:package && npm run build:styleguide",
    "clean": "shx rm -rf coverage/ dist/",
    "lint:eslint:fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix && npm run lint:stylelint:fix && npm run lint:markdown:fix",
    "lint:markdown": "markdownlint '**/*.md' --ignore node_modules",
    "lint:markdown:fix": "markdownlint '**/*.md' --ignore node_modules --fix",
    "lint:prettier:fix": "prettier -c --write '**/*.js'",
    "lint:prettier": "prettier -c '**/*.js'",
    "lint:stylelint:fix": "stylelint --fix '**/*.js'",
    "lint:stylelint": "stylelint '**/*.js'",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint && npm run lint:markdown",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "prepublish:styleguide-for-version": "npm run build",
    "prepublish:styleguide": "npm run build",
    "publish:styleguide-for-version": "PUBLISH_VERSION_FOLDER=true node bin/publish-gh-pages.js",
    "publish:styleguide": "node bin/publish-gh-pages.js",
    "start": "styleguidist server",
    "test:coverage:watch": "npm run test:coverage -- --watchAll",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:unit:update": "npm run test:unit -- --updateSnapshot",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit": "jest",
    "test": "npm run test:unit:watch"
  },
  "prettier": "@octopusthink/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octopusthink/nautilus.git"
  },
  "author": "Octopus Think",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octopusthink/nautilus/issues"
  },
  "homepage": "https://github.com/octopusthink/nautilus#readme",
  "dependencies": {
    "@reach/combobox": "^0.10.3",
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "emotion-theming": "^10.0.27",
    "feather-icons": "^4.28.0",
    "invariant": "^2.2.4",
    "prop-types": "^15.7.2",
    "shortid": "^2.2.15",
    "shx": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx-source": "^7.10.0",
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@octopusthink/eslint-config": "^0.5.0",
    "@octopusthink/prettier-config": "^0.5.2",
    "@reach/router": "^1.3.3",
    "@react-hook/window-size": "^1.0.13",
    "@testing-library/react": "^8.0.9",
    "acorn": "^6.3.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "history": "^4.10.1",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "jest-axe": "^3.4.0",
    "jest-dom": "^3.5.0",
    "jest-emotion": "^10.0.32",
    "lint-staged": "^10.2.6",
    "markdownlint-cli": "^0.23.1",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "prettier": "1.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-styleguidist": "^9.2.0",
    "shelljs": "^0.8.4",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.22",
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  }
}
