{
  "name": "@octopusthink/nautilus",
  "version": "0.4.0",
  "description": "Nautilus design system for React",
  "main": "dist/main.js",
  "module": "src/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:package && npm run build:styleguide",
    "build:package": "NODE_ENV=production webpack",
    "build:styleguide": "DISABLE_PEER_DEPS_PLUGIN=true NODE_ENV=production styleguidist build",
    "clean": "shx rm -rf coverage/ dist/",
    "ci": "npm run lint && npm run test:all",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix && npm run lint:stylelint:fix",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:prettier": "prettier -c 'src/**/*.js'",
    "lint:prettier:fix": "prettier -c --write 'src/**/*.js'",
    "lint:stylelint": "stylelint '**/*.js'",
    "lint:stylelint:fix": "stylelint --fix '**/*.js'",
    "prepare": "npm run build",
    "prepublish:styleguide": "npm run build",
    "publish:styleguide": "node bin/publish-gh-pages.js",
    "prepublish:styleguide-for-version": "npm run build",
    "publish:styleguide-for-version": "PUBLISH_VERSION_FOLDER=true node bin/publish-gh-pages.js",
    "start": "styleguidist server",
    "test": "jest --watch",
    "test:all": "jest",
    "test:all-with-coverage": "jest --coverage",
    "test:coverage": "jest --coverage --watchAll",
    "test:update": "jest --updateSnapshot"
  },
  "prettier": "@octopusthink/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octopusthink/nautilus.git"
  },
  "author": "Octopus Think",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octopusthink/nautilus/issues"
  },
  "homepage": "https://github.com/octopusthink/nautilus#readme",
  "dependencies": {
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "emotion-theming": "^10.0.19",
    "feather-icons": "^4.24.1",
    "invariant": "^2.2.4",
    "prop-types": "^15.7.2",
    "shortid": "^2.2.15",
    "shx": "^0.3.2"
  },
  "devDependencies": {
    "acorn": "^6.3.0",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@emotion/babel-preset-css-prop": "^10.0.23",
    "@emotion/core": "^10.0.22",
    "@octopusthink/eslint-config": "^0.5.0",
    "@octopusthink/prettier-config": "^0.5.2",
    "@reach/router": "^1.2.1",
    "@react-hook/window-size": "^1.0.8",
    "@testing-library/react": "^8.0.9",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.23",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "history": "^4.10.1",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "jest-dom": "^3.5.0",
    "jest-emotion": "^10.0.17",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "prettier": "1.17.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-styleguidist": "^9.2.0",
    "shelljs": "^0.8.3",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.0",
    "stylelint-processor-styled-components": "^1.7.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.22",
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  }
}
